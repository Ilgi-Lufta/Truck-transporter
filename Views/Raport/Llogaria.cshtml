@{
    ViewData["Title"] = "Home Page";
}
<partial name="NavPartial" />
<div class="mt-5 container">

    <div class="row">
        <div class="col sm-6">
            <table class="table table-hover table-sm table-info table-bordered  ">
                <thead>
                    <tr>
                        <th scope="col">Gjendja</th>
                        <th scope="col">GJendja Reale</th>
                        <th scope="col">Monedha</th>

                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in ViewBag.Totali)
                    {
                        <tr>
                            <td scope="row"> @item.Pagesa</td>
                            <td scope="row"> @item.PagesaReale</td>
                            <td scope="row"> @item.Currency.CurrencyUnit</td>

                        </tr>
                    }
                </tbody>
            </table>
            <div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col sm-6">
            <table class="table table-hover table-sm table-info table-bordered  ">
                <thead>
                    <tr>
                        <th scope="col">Pagesa</th>
                        <th scope="col">Litra</th>
                        <th scope="col">CmimRef</th>
                        <th scope="col">Monedha</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in ViewBag.naftaBlere2)
                    {
                        <tr>
                            <td scope="row"> @item.Pagesa</td>
                            <td scope="row"> @item.Litra</td>
                            <td scope="row"> @item.CmimRef</td>
                            <td scope="row"> @item.Monedha</td>

                        </tr>
                    }
                </tbody>
            </table>
            <div>
            </div>
        </div>
    </div>

</div>

